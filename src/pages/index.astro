---
import { AppConfig } from '../AppConfig'
import Home from '../components/Sections/Home.astro'
import Layout from '../layouts/Layout.astro'
import Message from '../assets/icons/message.svg'

// Sections

import SectionOne from '../components/Sections/SectionOne.astro'
import SectionTwo from '../components/Sections/SectionTwo.astro'
import SectionThree from '../components/Sections/SectionThree.astro'
import SectionFour from '../components/Sections/SectionThree.astro'
import SectionFive from '../components/Sections/SectionThree.astro'
import SectionSix from '../components/Sections/SectionThree.astro'

// Page Properties
const title = 'Home'
const description = AppConfig.DESCRIPTION
const image = AppConfig.IMAGE
const author = AppConfig.AUTHOR
---

<Layout
  {title}
  {description}
  {image}
  {author}>
  <Home title="Home" />
  <SectionOne title="Our Story" />
  <SectionTwo title="Product Features" />
  <SectionThree title="Product One" />
  <SectionFour title="Product Two" />
  <SectionFive title="Product Three" />
  <SectionSix title="Contact" />
</Layout>
